{
    "dart.runPubGetOnPubspecChanges": "never",
    "files.exclude": {
        "**/._*": true,
        "**/*.metadata": true,
        "**/*.packages": true,
        "**/*.lock": true,
        "**/*.dart_tool": true,
        "**/*.dart_frog": true,
        "**/*.idea": true,
        ".vscode": false,
        "**/*.iml": true,
        "**/*.current": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "env.properties": "*.properties, .current",
        "*.dart": "$(capture).g.dart, $(capture)_mixin.dart, $(capture)_helper.dart, $(capture).freezed.dart, $(capture).mapper.dart,",
        "pubspec.yaml": "*.yaml",
        ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
    },
}